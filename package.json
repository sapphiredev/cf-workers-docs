{
	"name": "@sapphire/slashies-handler",
	"version": "1.0.0",
	"description": "Slash Commands handler Sapphire Discord server",
	"author": "@sapphire <contact@sapphirejs.dev>",
	"license": "MIT",
	"private": true,
	"main": "dist/index.js",
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src",
		"test": "jest",
		"start": "node dist/index.js",
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"clean": "node scripts/clean.mjs",
		"update-slashies": "node ./scripts/interactions/updateSlashCommands.mjs",
		"update-guild-slashies": "node ./scripts/interactions/updateGuildSlashCommands.mjs",
		"update": "yarn up \"*\" -i",
		"validate-tags": "node scripts/validateTags.mjs",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"@discordjs/builders": "^0.6.0",
		"@discordjs/collection": "^0.2.1",
		"@ltd/j-toml": "^1.20.0",
		"@sapphire/fetch": "^2.0.1",
		"@sapphire/time-utilities": "^1.4.3",
		"@sapphire/utilities": "^3.0.3",
		"@skyra/jaro-winkler": "^1.0.0",
		"colorette": "^2.0.16",
		"discord-api-types": "^0.23.1",
		"discord.js-docs": "teeseal/discord.js-docs",
		"dotenv-cra": "^3.0.2",
		"fastify": "^3.22.0",
		"he": "^1.2.0",
		"tslib": "^2.3.1",
		"turndown": "^7.1.1",
		"tweetnacl": "^1.0.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.2.1",
		"@commitlint/config-conventional": "^13.2.0",
		"@sapphire/eslint-config": "^4.0.0",
		"@sapphire/prettier-config": "^1.2.2",
		"@sapphire/ts-config": "^3.1.1",
		"@types/he": "^1.1.2",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.10.3",
		"@types/turndown": "^5.0.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.0.0",
		"husky": "^7.0.2",
		"jest": "^27.2.5",
		"jest-circus": "^27.2.5",
		"lint-staged": "^11.2.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1",
		"ts-jest": "^27.0.5",
		"typescript": "^4.4.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/sapphire-slashies.git"
	},
	"bugs": {
		"url": "https://github.com/sapphiredev/sapphire-slashies/issues"
	},
	"homepage": "https://github.com/sapphiredev/sapphire-slashies",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"resolutions": {
		"ansi-regex": "^5.0.1"
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"packageManager": "yarn@3.0.2"
}
