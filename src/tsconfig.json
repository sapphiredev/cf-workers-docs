{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"rootDir": ".",
		"baseUrl": ".",
		"outDir": "../dist-tsc",
		"composite": true,
		"tsBuildInfoFile": "../dist-tsc/tsconfig.tsbuildinfo",
		"emitDeclarationOnly": true,
		"paths": {
			"#utils/*": ["lib/util/*"],
			"#types/*": ["lib/types/*"],
			"#constants/*": ["lib/constants/*"],
			"#api/*": ["lib/api/*"],
			"#env/*": ["lib/env/*"],
			"#select-menus/*": ["select-menus/*"],
			"#autocomplete/*": ["autocomplete/*"],
			"#commands/*": ["commands/*"],
			"#lib/*": ["lib/*"],
			"#root/*": ["*"]
		}
	},
	"include": ["."]
}
